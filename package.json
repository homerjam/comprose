{
  "name": "comprose",
  "version": "0.0.0",
  "description": "Compose written language in its ordinary form (data from faker.js)",
  "repository": {
    "url": "https://github.com/homerjam/comprose"
  },
  "author": "James Homer <james@homerjam.es>",
  "keywords": [
    "faker",
    "fake data",
    "seed",
    "random",
    "compose",
    "prose"
  ],
  "license": "MIT",
  "type": "module",
  "main": "dist/comprose.js",
  "module": "dist/comprose.mjs",
  "typings": "dist/comprose.d.ts",
  "files": [
    "dist/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint src/* test/*",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "test": "ava --watch test/*.spec.js",
    "dev": "concurrently -k 'npm run watch' 'npm run test'",
    "docs": "typedoc src/index.ts"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "ava": "^4.0.0",
    "concurrently": "^7.0.0",
    "esbuild": "^0.14.10",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "eslint-plugin-unicorn": "^40.0.0",
    "prettier": "^2.5.1",
    "rollup": "^2.63.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-esbuild": "^4.8.2",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4"
  },
  "ava": {
    "ignoredByWatcher": [
      "dist/**.*"
    ]
  },
  "eslintIgnore": [
    "dist/*.*"
  ],
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}